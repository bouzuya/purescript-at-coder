{
  "name": "purescript-at-coder",
  "description": "AtCoder",
  "version": "0.0.0",
  "author": {
    "url": "https://bouzuya.net/",
    "name": "bouzuya",
    "email": "m@bouzuya.net"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/purescript-at-coder/issues"
  },
  "dependencies": {
    "big-integer": "^1.6.43"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "purescript": "^0.12.3",
    "spago": "^0.7.5"
  },
  "homepage": "https://github.com/bouzuya/purescript-at-coder#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "repository": {
    "url": "git+https://github.com/bouzuya/purescript-at-coder.git",
    "type": "git"
  },
  "scripts": {
    "build": "spago --verbose bundle --main ${TASK}",
    "bundle": "parcel build --bundle-node-modules --no-autoinstall --no-source-maps --out-dir . --out-file bundle.js --target node index.js",
    "copy": "cat bundle.js | pbcopy",
    "docs": "spago docs",
    "env": "if [ -z ${TASK} ]; then exit 1; else exit 0; fi",
    "purs": "purs",
    "repl": "spago repl",
    "spago": "spago",
    "start": "npm-run-all -s env build bundle copy",
    "test": "spago test"
  }
}
